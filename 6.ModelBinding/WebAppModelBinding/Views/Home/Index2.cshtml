@model NameValueCollectionValueProvider
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table>
    <tr>
        <th colspan="4">foo</th>
    </tr>
    @foreach (var item in Model.GetKeysFromPrefix("first"))
    {
        <tr>
            <td>@item.Key</td>
            <td>@item.Value</td>
        </tr>
    }
    <tr>
        <th colspan="4">first[0]</th>
    </tr>
    @foreach (var item in Model.GetKeysFromPrefix("first[0]"))
    {
        <tr>
            <td>@item.Key</td>
            <td>@item.Value</td>
        </tr>
    }
    <tr>
        <th colspan="4">first[0].Address</th>
    </tr>
    @foreach (var item in Model.GetKeysFromPrefix("first[0].Address"))
    {
        <tr>
            <td>@item.Key</td>
            <td>@item.Value</td>
        </tr>
    }

    <tr>
        <th colspan="4">first[1]</th>
    </tr>
    @foreach (var item in Model.GetKeysFromPrefix("first[1]"))
    {
        <tr>
            <td>@item.Key</td>
            <td>@item.Value</td>
        </tr>
    }
</table>
